<h1>Gallery#index</h1>
<p>Find me in app/views/gallery/index.html.erb</p>


<ul>
	<% @images.each do |image_record|%>
	  <li class="gallery"><%= image_tag(image_record.file_name) %></li>
	  <p>Name: <%= image_record.name %></p>
	  <p>Description: <%= image_record.description%></p>
	<% end %>
</ul>
<style>
	img {width: 20%;
		}
</style>